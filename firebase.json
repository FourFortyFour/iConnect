{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "site": "iconnectae",
    "source": "iconnect-frontend",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1",
      "minInstances": 1
    },
    "rewrites": [
      {
        "source": "**",
        "function": "paymentwebhook",
        "region": "europe-west1"
      }
    ]
  }
}